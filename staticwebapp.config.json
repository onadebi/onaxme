{
    "routes": [
      {
        "route": "/api/*",
        "methods": ["GET", "POST", "PUT", "DELETE"],
        "allowedRoles": ["anonymous"]
      },
      {
        "route": "/*",
        "methods": ["GET"],
        "rewrite": "/"
      }
    ],
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
    },
    "platform": {
      "apiRuntime": "node:20"
    }
  }